{
  "expo": {
    "name": "EmployeeApp",
    "slug": "EmployeeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/whatsapp5.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/whatsapp5.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to verify you are at the office for attendance.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We use your location to verify you are at the office for attendance."
      },
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/whatsapp5.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "usesCleartextTraffic": true,
      "package": "com.vienstereoptic.EmployeeApp"
    },
    "web": {
      "favicon": "./assets/whatsapp5.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "isAndroidBackgroundLocationEnabled": false
        }
      ],
      "expo-asset"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8acc278c-b52f-4860-b6b2-683d4f56c110"
      }
    },
    "owner": "solomon_king"
  }
}
